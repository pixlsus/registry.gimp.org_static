    <div id="node-24754" class="node node-scriptfu node-promoted">

  
      
      <span class="submitted">September 26, 2010 - 21:42 — <span class="username">Ofnuts</span></span>
  
  <div class="content clearfix">
    <div class="form-item form-type-item">
  <label>Rate </label>
 <div class="rate-widget-2 rate-widget clear-block rate-average rate-widget-fivestar rate-2fca2e7eb0bd5482025d742949bda9fd rate-node-24754-2-3" id="rate-node-24754-2-3"><div class="item-list"><ul><li class="first"><span class="rate-button rate-fivestar-btn-empty rate-fivestar-1" id="rate-button-1">1</span></li>
<li><span class="rate-button rate-fivestar-btn-empty rate-fivestar-2" id="rate-button-2">2</span></li>
<li><span class="rate-button rate-fivestar-btn-empty rate-fivestar-3" id="rate-button-3">3</span></li>
